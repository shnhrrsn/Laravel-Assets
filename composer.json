{
	"name": "shnhrrsn/laravel-assets",
	"description": "Asset management for Laravel.",
	"license": "MIT",
	"authors": [
		{
			"name": "Shaun Harrison",
			"email": "mail@shnhrrsn.com"
		}
	],
	"require": {
		"php": ">=5.4.0",
		"illuminate/console": "5.2.*",
		"illuminate/support": "5.2.*",
		"symfony/process": "2.8.*|3.0.*",
		"symfony/finder": "2.*"
	},
	"autoload": {
		"psr-4": {
			"Assets\\": "src/"
		},
		"files": [
			"src/helpers.php"
		]
	},
	"suggest": {
		"laravelcollective/html": "Allows Asset to extend HtmlBuilder and add support for `HTML::assetPath()`"
	},
	"minimum-stability": "dev"
}
